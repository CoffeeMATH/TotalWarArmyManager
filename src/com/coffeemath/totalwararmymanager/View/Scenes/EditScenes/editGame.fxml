<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane stylesheets="@../../StyleSheets/editGameCSS.css"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.coffeemath.totalwararmymanager.Controller.PopUps.EditGameController"
            prefHeight="150.0" prefWidth="250.0">
    <children>
        <TextArea fx:id="nameField" layoutX="28.0" layoutY="59.0" prefHeight="33.0" prefWidth="194.0" />
        <Button fx:id="addButton" onAction="#editTheGame" layoutX="91.0" layoutY="98.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="61.0" text="Done">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Label layoutX="28.0" layoutY="14.0" text="Enter Name:">
            <font>
                <Font size="17.0" />
            </font>
        </Label>
    </children>
</AnchorPane>

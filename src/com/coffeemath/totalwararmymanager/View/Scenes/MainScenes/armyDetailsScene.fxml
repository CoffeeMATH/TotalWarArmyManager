<VBox fx:id="root" maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.coffeemath.totalwararmymanager.Controller.ChooseArmySceneController">
    <children>
      <HBox prefHeight="44.0" prefWidth="600.0">
         <children>
            <Label fx:id="playerLabel" text="" />

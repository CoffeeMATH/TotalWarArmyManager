            <Label fx:id="gameLabel" text="" />
             <Label text="-&gt;" />
             <Label fx:id="armyLabel" text="" />
         </children></HBox>
      <HBox prefHeight="44.0" prefWidth="600.0" />
        <TableView fx:id="armies" prefHeight="370.0" prefWidth="600.0" />
        <HBox prefHeight="44.0" prefWidth="600.0">
            <children>
                <Button fx:id="addBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Add Army" textAlignment="CENTER" />

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" maxHeight="400.0" maxWidth="600.0" prefHeight="400.0" prefWidth="600.0" stylesheets="@../../StyleSheets/chooseScenesCSS.css" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.coffeemath.totalwararmymanager.Controller.ChooseGameSceneController">
   <children>
      <HBox prefHeight="45.0" prefWidth="600.0">
         <children>
            <Label fx:id="playerLabel" />
         </children>
      </HBox>
      <TableView fx:id="games" prefHeight="370.0" prefWidth="600.0" />
      <AnchorPane prefHeight="56.0" prefWidth="600.0">
         <children>
            <Button fx:id="backBtn" layoutY="2.0" mnemonicParsing="false" text="Back" />
            <Button fx:id="addBtn" layoutX="525.0" layoutY="1.0" mnemonicParsing="false" text="Add Game" />
         </children>
      </AnchorPane>
   </children>
</VBox>

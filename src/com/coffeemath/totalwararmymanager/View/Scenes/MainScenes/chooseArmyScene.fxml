<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" stylesheets="@../../StyleSheets/chooseScenesCSS.css" maxHeight="1000" maxWidth="1000" minHeight="400.0" minWidth="600.0" prefHeight="0" prefWidth="0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.coffeemath.totalwararmymanager.Controller.ChooseArmySceneController">
    <children>
      <HBox prefHeight="44.0" prefWidth="600.0">
         <children>
            <Label fx:id="playerLabel" text="" />
            <Label text="-&gt;" />
            <Label fx:id="gameLabel" text="" />
         </children></HBox>
        <TableView fx:id="armies" prefHeight="370.0" prefWidth="600.0" />
        <HBox prefHeight="44.0" prefWidth="600.0" spacing="800">
            <children>
                <Button fx:id="addBtn" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Add Army" textAlignment="CENTER" prefHeight="100" prefWidth="250"/>
                <Button fx:id="backBtn" mnemonicParsing="false" text="Back" prefHeight="100" prefWidth="250"/>
            </children>
        </HBox>
    </children>
</VBox>
